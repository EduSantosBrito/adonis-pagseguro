{
	"extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "error",
		"global-require": "off",
		"import/no-extraneous-dependencies": "off",
		"node/no-unpublished-require": "off"
	},
	"env": {
		"node": true,
		"es6": true
	},
}
